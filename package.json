{
  "name": "openmeet",
  "version": "0.0.2",
  "description": "Building Communities",
  "productName": "OpenMeet",
  "author": "Stan Khatko <stanislavkhatko@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "dev": "QENV=development quasar dev",
    "build": "QENV=production quasar build",
    "deploy": "vercel --prod",
    "test": "yarn test:unit:ci && yarn test:e2e:ci",
    "test:unit:ui": "QENV=test vitest --ui",
    "test:unit": "QENV=test vitest",
    "test:unit:ci": "QENV=test vitest run",
    "test:e2e": "cross-env QENV=test start-test \"quasar dev -p 8087\" http-get://127.0.0.1:8087 \"COVERAGE=true cypress open --e2e\"",
    "test:e2e:ci": "cross-env QENV=test start-test \"quasar dev -p 8087\" http-get://127.0.0.1:8087 \"COVERAGE=true cypress run --e2e\" && npx nyc report --reporter=lcov --reporter=text",
    "test:component": "cross-env QENV=test COVERAGE=true cypress open --component",
    "test:component:ci": "cross-env QENV=test COVERAGE=true cypress run --component && npx nyc report --reporter=lcov --reporter=text",
    "test:coverage": "npx nyc report --reporter=lcov --reporter=text"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.13",
    "@types/dompurify": "^3.0.5",
    "axios": "^1.2.1",
    "dompurify": "^3.1.7",
    "dotenv": "^16.4.5",
    "leaflet": "^1.9.4",
    "pinia": "^2.2.6",
    "posthog-js": "^1.181.0",
    "qrcode": "^1.5.4",
    "quasar": "^2.17.2",
    "vue": "^3.4.18",
    "vue-advanced-cropper": "^2.8.9",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@pinia/testing": "^0.1.7",
    "@quasar/app-vite": "^2.0.0-beta.12",
    "@quasar/quasar-app-extension-qcalendar": "^4.0.0-beta.16",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^6.1.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.1.0",
    "@types/leaflet": "^1.9.12",
    "@types/node": "^22.9.0",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitest/ui": "^2.1.4",
    "@vue/test-utils": "^2.4.4",
    "autoprefixer": "^10.4.2",
    "cypress": "^13.15.2",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.30.0",
    "typescript": "~5.5.3",
    "vite-plugin-checker": "^0.7.2",
    "vite-plugin-istanbul": "^6.0.2",
    "vitest": "^2.1.4",
    "vue-router-mock": "^1.1.0",
    "vue-tsc": "2.0.29"
  },
  "engines": {
    "node": "^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
