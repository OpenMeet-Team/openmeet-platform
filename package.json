{
  "name": "openmeet-platform",
  "version": "0.2.0",
  "description": "OpenMeet frontend platform for community event management and real-time chat",
  "productName": "OpenMeet",
  "author": "OpenMeet Team <openmeet@openmeet.net>",
  "type": "module",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "dev": "QENV=development quasar dev",
    "build": "QENV=production quasar build",
    "deploy": "vercel --prod",
    "test": "yarn test:unit:ci && yarn test:e2e:ci",
    "test:unit:ui": "QENV=test vitest --ui",
    "test:unit": "QENV=test vitest",
    "test:unit:ci": "CI=true QENV=test vitest run",
    "test:e2e": "cross-env QENV=test start-test \"quasar dev -p 8087\" http-get://127.0.0.1:8087 \"COVERAGE=true cypress open --e2e\"",
    "test:e2e:ci": "cross-env CI=true QENV=test start-test \"quasar dev -p 8087\" http-get://127.0.0.1:8087 \"COVERAGE=true cypress run --e2e\"",
    "test:e2e:single": "cross-env CI=true QENV=test COVERAGE=true npx start-test \"quasar dev -p 8087\" http-get://127.0.0.1:8087 \"cypress run --e2e --spec $CYPRESS_SPEC\"",
    "test:component": "cross-env QENV=test COVERAGE=true cypress open --component",
    "test:component:ci": "cross-env QENV=test COVERAGE=true cypress run --component",
    "test:coverage": "npx nyc report --reporter=lcov --reporter=text"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/context-zone": "^1.30.1",
    "@opentelemetry/instrumentation-document-load": "^0.44.0",
    "@opentelemetry/instrumentation-user-interaction": "^0.44.0",
    "@opentelemetry/web": "^0.24.0",
    "@quasar/extras": "^1.16.13",
    "@types/dompurify": "^3.0.5",
    "@types/marked": "^5.0.2",
    "axios": "^1.12.2",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dompurify": "^3.2.6",
    "dotenv": "^16.4.5",
    "leaflet": "^1.9.4",
    "marked": "^15.0.7",
    "matrix-encrypt-attachment": "^1.0.3",
    "matrix-js-sdk": "^38.2.0",
    "pinia": "^2.2.6",
    "posthog-js": "^1.252.0",
    "qrcode": "^1.5.4",
    "quasar": "^2.17.2",
    "rrule": "^2.8.1",
    "vue": "^3.4.18",
    "vue-advanced-cropper": "^2.8.9",
    "vue-i18n": "^9.14.4",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@babel/runtime": "^7.26.10",
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@pinia/testing": "^0.1.7",
    "@quasar/app-vite": "^2.2.1",
    "@quasar/quasar-app-extension-qcalendar": "^4.0.0-beta.16",
    "@quasar/quasar-app-extension-qmarkdown": "^2.0.5",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^6.2.1",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.2.3",
    "@types/leaflet": "^1.9.12",
    "@types/node": "^22.9.0",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitest/ui": "^3.2.3",
    "@vue/test-utils": "^2.4.4",
    "autoprefixer": "^10.4.2",
    "axios-mock-adapter": "^2.1.0",
    "cypress": "^13.2.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.30.0",
    "typescript": "~5.5.3",
    "vite-plugin-checker": "^0.7.2",
    "vite-plugin-istanbul": "^6.0.2",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^3.2.3",
    "vue-router-mock": "^1.1.0",
    "vue-tsc": "2.0.29"
  },
  "engines": {
    "node": "^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
