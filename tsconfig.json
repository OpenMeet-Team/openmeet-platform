{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "types": [
      "cypress"
    ],
    "allowJs": true,
    "moduleResolution": "node",
    "module": "ESNext"
  },
  "esModuleInterop": true,
  "exclude": [
    "./local",
    "./dist",
    "./.quasar",
    "./node_modules",
    "./src-capacitor",
    "./src-cordova",
    "./quasar.config.*.temporary.compiled*"
  ]
}
